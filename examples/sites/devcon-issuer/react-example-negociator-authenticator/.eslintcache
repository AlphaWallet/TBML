nsole.log(error);\n      });\n    }, (error) => {\n      console.log(error);\n    });\n  }, []);\n  return (\n    <div>\n      <Modal></Modal>\n      <div style={{ flexWrap: 'wrap', display: 'flex', justifyContent: 'center', margin: '60px' }}>\n        {tokens.length > 0 && tokens.map((tokenInstance, index) => {\n          return <Card key={index} tokenInstance={tokenInstance} selectVIPEventHandler={selectVIPEventHandler} />\n        })\n        }\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/nicktaras/Documents/github/TokenScript/examples/sites/devcon-issuer/react-example/src/Card/index.js",[],"/Users/nicktaras/Documents/github/TokenScript/examples/sites/devcon-issuer/react-example/src/index.js",[],"/Users/nicktaras/Documents/github/TokenScript/examples/sites/devcon-issuer/react-example/src/TokenScript/contractMock.js",[],"/Users/nicktaras/Documents/github/TokenScript/examples/sites/devcon-issuer/react-example/src/TokenScript/ContractMock.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","severity":1,"message":"94","line":5,"column":11,"nodeType":"95","messageId":"96","endLine":5,"endColumn":17},{"ruleId":"97","severity":2,"message":"98","line":14,"column":12,"nodeType":"95","messageId":"99","endLine":14,"endColumn":19},{"ruleId":"100","severity":1,"message":"101","line":1,"column":1,"nodeType":"102","endLine":5,"endColumn":3},{"ruleId":"97","severity":2,"message":"103","line":37,"column":3,"nodeType":"95","messageId":"99","endLine":37,"endColumn":24},{"ruleId":"104","severity":1,"message":"105","line":40,"column":16,"nodeType":"106","messageId":"107","endLine":40,"endColumn":18},{"ruleId":"108","severity":1,"message":"109","line":66,"column":6,"nodeType":"110","endLine":66,"endColumn":8,"suggestions":"111"},{"ruleId":"100","severity":1,"message":"101","line":1,"column":1,"nodeType":"102","endLine":5,"endColumn":3},{"ruleId":"104","severity":1,"message":"105","line":36,"column":16,"nodeType":"106","mess